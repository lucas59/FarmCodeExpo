{
  "expo": {
    "name": "Etiqueta Virtual",
    "slug": "farmcode",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "20d57323-820c-49bd-85bb-6732c30bb4b9"
      }
    },
    "platforms": ["ios", "android", "web"],
    "version": "6.0.0",
    "jsEngine": "hermes",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "icon": "./assets/solidIcon.png",

    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.gs1uy.etiquetavirtual",
      "buildNumber": "5.4.1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
      }
    },
    "android": {
      "package": "com.etiqueta.virtual",
      "versionCode": 10
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "ts", "tsx"]
    }
  },
  "plugins": [
    [
      "expo-build-properties",
      {
        "android": {
          "compileSdkVersion": 34,
          "targetSdkVersion": 34,
          "buildToolsVersion": "34.0.0"
        }
      }
    ]
  ]
}
